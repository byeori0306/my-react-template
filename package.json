{
    "name": "my-react-template",
    "version": "1.0.0",
    "description": "직접 React 개발환경 구성해보기 (with Emotion)",
    "scripts": {
        "start": "webpack serve --config webpack/webpack.config.js --open --env env=development --progress",
        "build": "webpack --config webpack/webpack.config.js --env env=production",
        "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
        "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com-byeori0306:byeori0306/my-react-template.git"
    },
    "keywords": [
        "React template"
    ],
    "author": "byeori0306@gmail.com",
    "license": "ISC",
    "dependencies": {
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mui/material": "^5.11.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@babel/runtime": "^7.20.7",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
        "@svgr/webpack": "^6.5.1",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.10",
        "@typescript-eslint/eslint-plugin": "^5.48.0",
        "@typescript-eslint/parser": "^5.48.0",
        "babel-loader": "^9.1.2",
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^6.7.3",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "eslint": "^8.31.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
